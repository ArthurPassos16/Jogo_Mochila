#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include "leituras.h"

int main(){
    Objeto *objeto=NULL;
    Mochila *mochila=NULL;

    objeto=leituraObjeto();
    mochila=leituraMochila();
    srand(time(NULL));
    int opcao, cargaBolsa;
    do{
        printf("1 - JOGAR\n2 - RECORDES\n3 - SAIR\n");
        scanf("%d",&opcao);
        switch(opcao){
            case 1:
                printf("JOGO\n");
                for(int i=0;i<mochila->num_itens;i++){
                  printf("Peso: %d\n",objeto[i].peso);
                  printf("Valor: %d\n",objeto[i].valor);
                }
            break;
            case 2:
            break;
            case 3:
                printf("JOGO ENCERRADO!\n");
            break;
            default:
                printf("Opcao Invalida!\n");
            break;
        }
    }while(opcao!=3);
    return 0;
}
